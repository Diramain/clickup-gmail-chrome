{
  "name": "clickup-gmail-chrome",
  "version": "1.0.0",
  "description": "Chrome extension to create ClickUp tasks from Gmail",
  "private": true,
  "scripts": {
    "build": "tsc && node build.js",
    "build:js": "node build.js",
    "watch": "tsc -w & node build.js --watch",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.260",
    "esbuild": "^0.19.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "typescript": "^5.9.3"
  }
}
